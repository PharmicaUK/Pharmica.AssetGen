<Project>
  <PropertyGroup>
    <!-- Default value for AssetGen_ClassName -->
    <AssetGen_ClassName Condition="'$(AssetGen_ClassName)' == ''">StaticAssets</AssetGen_ClassName>
  </PropertyGroup>
  <ItemGroup>
    <!-- Make properties available to the source generator -->
    <CompilerVisibleProperty Include="AssetGen_ClassName" />
    <CompilerVisibleProperty Include="AssetGen_FlattenExtensions" />
    <CompilerVisibleProperty Include="AssetGen_PathBase" />
  </ItemGroup>
</Project>
